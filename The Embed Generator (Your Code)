const generateEmbedCode = (playbackId) => {
  const embedUrl = `https://your-clone-app.com/embed/${playbackId}`;
  
  return `
<div style="position:relative;padding-top:177.77%;width:100%;max-width:450px;margin:auto;">
  <iframe 
    src="${embedUrl}" 
    loading="lazy"
    style="position:absolute;top:0;left:0;width:100%;height:100%;border:none;border-radius:12px;shadow:0 10px 15px -3px rgba(0,0,0,0.1);"
    allow="autoplay; fullscreen; picture-in-picture"
    title="Titan-Tok Video Player"
  ></iframe>
</div>`.trim();
};
